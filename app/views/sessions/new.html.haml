= form_for(:session, :url => sessions_path) do |f|
  -# <!-- %= render 'shared/error_messages', :object => f.object % -->
  .field
    = f.label :email
    %br
    = f.text_field :email
  .field
    = f.label :password
    %br
    = f.password_field :password
  .actions
    = f.submit "Sign in"

%p New User? #{link_to 'Sign up now!' ,signup_path}

